{
    "dataform-version": "1.0.0",
    "type": "gridpanel",
    "showLabels": true,
    "cols": 1,
    "children": [
        {
            "type": "textbox",
            "field": "title",
            "label": "${dataForm.title}",
            "tooltip": "${dataForm.title}",
            "required": true,
            "size": {
                "w": 300
            }
        },
        {
            "type": "textarea",
            "field": "infoText",
            "label": "${dataForm.infoText}",
            "tooltip": "${dataForm.infoText}",
            "required": true,
            "size": {
                "w": 300,
                "h": 100
            }
        },
        {
            "type": "textbox",
            "field": "buttonText",
            "label": "${dataForm.buttonText}",
            "tooltip": "${dataForm.buttonText}",
            "size": {
                "w": 300
            }
        },
        {
            "type": "textbox",
            "field": "checkboxText",
            "label": "${dataForm.checkboxText}",
            "tooltip": "${dataForm.checkboxText}",
            "size": {
                "w": 300
            }
        },
        {
            "type": "textbox",
            "field": "imgUrl",
            "label": "${dataForm.imgUrl}",
            "tooltip": "${dataForm.imgUrl}",
            "size": {
                "w": 300
            }
        },
        {
            "type": "textbox",
            "field": "imgHeight",
            "label": "${dataForm.imgHeight}",
            "tooltip": "${dataForm.imgHeight}",
            "size": {
                "w": 300
            }
        },
        {
            "type": "checkbox",
            "field": "accept",
            "label": "${dataForm.enableAcceptance}",
            "tooltip": "${dataForm.enableAcceptance}",
            "size": {
                "w": 300
            }
        },
        {
            "type": "numberspinner",
            "field": "expirationTime",
            "label": "${dataForm.expirationTime}",
            "tooltip": "${dataForm.expirationTime}",
            "min": 1,
            "size": {
                "w": 300
            }
        }
    ]
}
